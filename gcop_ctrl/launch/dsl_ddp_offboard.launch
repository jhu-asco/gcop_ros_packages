<?xml version="1.0"?>
<launch>
  <!-- Displays the hopkins satellite image -->
  <include file="$(find gcop_ros_est)/launch/satmap_server.launch"/>

  <!-- Displays the occ grid map obtained from gmapping -->
  <node name='map1_server' pkg='map_server' type='map_server' args='$(find gcop_ctrl)/map/map_latrobe_400.yaml' output="screen">
    <param name='frame_id' value='og_org'/>
  </node>
  
  <!-- Transformation between satellite map and occ tree map -->
  <node pkg="tf" type="static_transform_publisher" name="world_to_og_org" args="103 244 0 1.30899694 0 0 world og_org 50" />
  
 <!-- Dynamic reconfigure -->
 <node pkg="rqt_reconfigure" type="rqt_reconfigure" name="reconfigure" output="screen" />

  <!-- Rviz node -->
   <node name="rviz" pkg="rviz" type="rviz" args="-d $(find gcop_ctrl)/dsl_ddp.rviz" output="screen"/> 
</launch>

